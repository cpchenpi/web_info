                var $dialog = $('#tv-play-source');
                var $this = $(this);
                source = $this.data('source');
                if(!source)return;
                cn = $this.data('cn');

                tmpl = $.templates('#episode-tmpl');
                $dialog = $(tmpl.render({playlist: sources[source], cn: cn}));

                $dialog.hide();
                $('body').append($dialog);
                width = $dialog.outerWidth();

                $dialog.css({
                    marginLeft: -width / 2,
                    top: $this.offset().top + $this.outerHeight()
                }).show();
            });
        });
    </script>

