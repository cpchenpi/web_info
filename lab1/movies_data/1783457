        (function(){
            $("body").delegate(".comment-item", 'mouseenter mouseleave', function (e) {
                switch (e.type) {
                    case "mouseenter":
                    $(this).find(".comment-report").css('visibility', 'visible');
                    break;
                    case "mouseleave":
                    $(this).find(".comment-report").css('visibility', 'hidden');
                    break;
                }
            });
        })()
    </script>

