                }
            }

            $('.indent .a_show_full').click(function(){
                $.get('/blank?expand');
            });

            $('body').on('click', 'a.write_review', function(e) {
                e.preventDefault();
                if(window._USER_ABNORMAL) {
                    show_abnormal && show_abnormal()
                }else {
                    location.href = $(this).attr('href')
                }
            })
        });
    </script>
    
    




    
