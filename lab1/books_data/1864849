<script>
  $(document).ready(function() {
    $('.impression_track_mod_buyinfo').each(function(i, item) {
      if (item) {
        var itmbUrl = $(item)[0]['dataset']['track']
        reportTrack(itmbUrl)
      }
    })
  })

  function track(url) {
    reportTrack(url)
  }

  function reportTrack(url) {
    if (!url) { return false }
    $.ajax({ url: url, dataType: 'text/html' })
  }
</script>













  
    






  

