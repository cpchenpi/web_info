                var $this = $(e.target),
                    $source = $('.play-source');
                if (!$this.is('.playBtn') && !$this.parents('.play-source').length) {
                    $source.remove();
                }
            });
            var sources = {};
            sources[13] = [
                        {play_link: "https://www.douban.com/link2/?url=http%3A%2F%2Fvip.1905.com%2Fplay%2F454310.shtml%3F__hz%3D6e0721b2c6977135%26api_source%3Ddouban_vodadd&amp;subtype=13&amp;type=online-video", ep: "1"},
