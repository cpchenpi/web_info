                var $dialog = $this.parents('#tv-play-source');
                $dialog.remove();
            });
            $('body').bind('click', function(e) {
                var $this = $(e.target),
                    $source = $('.play-source');
                if (!$this.is('.playBtn') && !$this.parents('.play-source').length) {
                    $source.remove();
                }
            });
            var sources = {};
            sources[15] = [
                        {play_link: "https://www.douban.com/link2/?url=https%3A%2F%2Fm.miguvideo.com%2Fmgs%2Fmsite%2Fprd%2Fdetail.html%3Fcid%3D725076248%26pwId%3Dd01197d3076b4164af82983c408bb996&amp;subtype=15&amp;type=online-video&amp;link2key=ac116de602", ep: "1"},
