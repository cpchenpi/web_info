                        SUBJECT_COMMENTS_SECTION.createVoteHandler({
                            api: '/j/comment/vote',
                            root: document.getElementById('comments-section'),
                            voteSelector: '.vote-comment',
                            textSelector: '.vote-count',
                            is_released: "true",
                            alert_text: "该电影还未上映，不能投票噢",
